pipeline {
  agent any
  stages {
    stage('Plan') {
      steps {
        echo 'plan for pipeline'
      }
    }

    stage('Build') {
      steps {
        echo 'build code for pipeline'
      }
    }

    stage('test') {
      steps {
        echo 'test code for pipeline'
      }
    }

    stage('deploy') {
      steps {
        echo 'Deploy code for pipeline'
      }
    }

  }
}